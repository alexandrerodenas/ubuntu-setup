# Alias block start -- Do not remove or modify this line --

alias ealias='vim ~/.bash_aliases'
alias salias='source ~/.bash_aliases'

# Basic commands
alias ..='cd .. && ls -lh'
alias ls='ls -lah'
alias df='df -Tha --total'
alias du='du -ach | sort -h'
alias ps='ps auxf'
alias wget='wget -c'
alias top='htop'
alias home='cd ~'

# Customs
alias dev="cd ~/dev"
